{
  "name": "stm32_usb_hid_tool",
  "version": "0.1.0",
  "dev": true,
  "description": "stm32 usb hid demonstration tool",
  "author": "Radek Ripa",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Araw86/stm32_usb_hid_tool"
  },
  "main": "./build/main/main.js",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "^6.1.9",
    "@reduxjs/toolkit": "^2.5.1",
    "electron-redux": "^2.0.0",
    "node-hid": "^3.2.0",
    "pdf2json": "~2.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.4"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "electron-devtools-installer": "^4.0.0",
    "electron-dl": "^3.5.0",
    "electron-is-dev": "^3.0.1",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.9",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "scripts": {
    "start": "electron .",
    "watch": "webpack --config webpack.dev.js --watch",
    "build_dev": "webpack --config webpack.dev.js",
    "build_prod": "webpack --config webpack.prod.js",
    "build_dist": "webpack --config webpack.prod.js && electron-builder -c.extraMetadata.main=build/main/main.js",
    "build_dist_only": "electron-rebuild -f -w node-hid && electron-builder -c.extraMetadata.main=build/main/main.js --win",
    "build_dist_github": "yarn build_dist_only --win --publish always"
  },
  "build": {
    "extends": null,
    "appId": "app.stm32_usb_hid_tool",
    "productName": "stm32 usb hid tool",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extraFiles": [
      {
        "from": "./build/database/",
        "to": "database",
        "filter": [
          "*"
        ]
      }
    ],
    "win": {
      "publish": {
        "provider": "github",
        "owner": "Araw86",
        "repo": "stm32_usb_hid_tool"
      }
    },
    "publish": {
      "provider": "github",
      "owner": "Araw86",
      "repo": "stm32_usb_hid_tool"
    }
  },
  "packageManager": "yarn@3.6.1+sha512.de524adec81a6c3d7a26d936d439d2832e351cdfc5728f9d91f3fc85dd20b04391c038e9b4ecab11cae2b0dd9f0d55fd355af766bc5c1a7f8d25d96bb2a0b2ca"
}
